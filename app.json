{
   "colors": {
      "primaryColor": "#5E49FF",
      "greenColor": "#3BD42D",
      "greenWaterColor": "#25CBD3",
      "MediumSpringGreen": "#00FA9A",
      "lightBlueColor": "#F0F5FF",
      "whiteColor": "#FFFFFF",
      "grayColor00": "#F4F4F4",
      "grayColor01": "#DADADA",
      "grayColor02": "#7C7786",
      "grayColor03": "#4D4C4D",
      "grayColor04": "#333333",
      "grayColor05": "#E5E5E5",
      "redColor": "#DC143C"
    },
  "expo": {
    "name": "rede_social_react_native",
    "slug": "rede_social_react_native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.gildevdevdev.rede_social_react_native"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo RedeSocial, deseja ter acesso a galeria do seu dispositivo."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ef19d925-22e1-41c9-aa59-68f94aa5a31f"
      }
    },
    "owner": "gildevdevdev"
  }
}